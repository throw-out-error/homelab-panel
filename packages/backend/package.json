        "swagger-ui-express": "4.1.6",
        "typeorm": "0.2.31"
    },
    "devDependencies": {
        "@nestjs/cli": "7.5.4",
        "@nestjs/schematics": "7.2.7",
        "@nestjs/testing": "7.6.12",
        "@throw-out-error/pm": "workspace:*",
        "@throw-out-error/homelab-common": "workspace:*",
        "@types/express": "4.17.11",
        "@types/forever-monitor": "1.7.5",
        "@types/jest": "26.0.15",
        "@types/node": "14.14.25",
        "@types/nodegit": "0.26.12",
        "@types/supertest": "2.0.10",
        "@typescript-eslint/eslint-plugin": "4.8.2",
        "@typescript-eslint/parser": "4.8.2",
        "eslint": "7.14.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.3.1",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "supertest": "5.0.0",
        "ts-jest": "26.4.4",
        "ts-loader": "8.0.17",
        "ts-node": "9.0.0",
        "tsconfig-paths": "3.9.0",
        "typescript": "4.1.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
